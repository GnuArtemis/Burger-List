<li class="valign-wrapper">
    <span>{{burger_name}}</span>

    <button class="change-devoured waves-effect waves-light btn-small" data-id="{{this.id}}" data-newdevoured="{{devoured}}">
        {{#if devoured}}EAT ME!{{else}}Order another?{{/if}}
    </button>


    
    
	<button class="delete-burger waves-effect waves-light btn-small" data-id="{{this.id}}">No more!</button>
</li>